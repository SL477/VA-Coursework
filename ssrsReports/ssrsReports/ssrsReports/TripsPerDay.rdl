  <DataSets>
    <DataSet Name="Data">
      <Query>
        <DataSourceName>BikeData</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@startDay">
            <Value>=Parameters!startDay.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@endDay">
            <Value>=Parameters!endDay.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        StartDay, trips
FROM            tripsPerDay
WHERE StartDay BETWEEN @startDay AND @endDay
ORDER BY StartDay</CommandText>
      </Query>
      <Fields>
        <Field Name="StartDay">
          <DataField>StartDay</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="trips">
          <DataField>trips</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="MinMax">
      <Query>
        <DataSourceName>BikeData</DataSourceName>
        <CommandText>SELECT MIN(StartDay) AS MinDay, MAX(StartDay) AS MaxDay FROM tripsPerDay</CommandText>
      </Query>
      <Fields>
        <Field Name="MinDay">
          <DataField>MinDay</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="MaxDay">
          <DataField>MaxDay</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
